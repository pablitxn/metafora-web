{
  "name": "metafora-web",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "lint:css": "stylelint './src/**/styles.ts'",
    "lint:ts": "eslint src --ext .ts,.tsx",
    "start": "next start",
    "prepare": "husky install"
  },
  "dependencies": {
    "next": "12.1.5",
    "postcss-syntax": "^0.36.2",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-feather": "2.0.9",
    "styled-components": "5.3.5"
  },
  "devDependencies": {
    "@commitlint/cli": "16.2.3",
    "@commitlint/config-conventional": "16.2.1",
    "@stylelint/postcss-css-in-js": "^0.37.2",
    "@types/next": "9.0.0",
    "@types/react": "18.0.5",
    "@types/react-dom": "18.0.0",
    "@types/styled-components": "5.1.25",
    "@typescript-eslint/eslint-plugin": "5.19.0",
    "@typescript-eslint/parser": "5.19.0",
    "eslint": "8.13.0",
    "eslint-config-airbnb-typescript-prettier": "5.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.29.4",
    "eslint-plugin-styled-components": "0.0.0",
    "eslint-plugin-styled-components-a11y": "0.0.34",
    "eslint-plugin-styled-components-config": "0.0.2",
    "husky": "7.0.4",
    "lint-staged": "12.3.7",
    "prettier": "2.6.2",
    "standard-version": "9.3.2",
    "stylelint": "14.6.1",
    "stylelint-a11y": "1.2.3",
    "stylelint-config-recommended": "7.0.0",
    "stylelint-config-standard": "25.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.10.0",
    "typescript": "4.6.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "src/**/*.{js,ts,tsx,json,css,md}": [
      "prettier --write",
      "eslint"
    ]
  },
  "resolutions": {
    "styled-components": "5"
  }
}
