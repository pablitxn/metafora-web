{
  "name": "metafora-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "jest",
    "test-ut": "react-scripts test --runner=groups --group=unit ",
    "test-co": "react-scripts test --coverage --watchAll=false",
    "test-ci": "react-scripts test --runner=groups --group=unit --watchAll=false --coverage --testResultsProcessor=jest-junit"
  },
  "dependencies": {
    "@ant-design/icons": "^4.4.0",
    "@auth0/nextjs-auth0": "^1.3.0",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^12.7.1",
    "@zeit/next-less": "^1.0.1",
    "antd": "^4.11.2",
    "babel-plugin-import": "^1.13.3",
    "less": "^4.1.0",
    "less-vars-to-js": "^1.3.0",
    "next": "10.0.6",
    "null-loader": "^4.0.1",
    "prettier": "^2.2.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-feather": "^2.0.9",
    "webpack": "^4.19.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.22",
    "@types/react": "^17.0.0",
    "jest": "^26.6.3",
    "jest-junit": "^12.0.0",
    "jest-runner-groups": "^2.0.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/path/to/dir/"
    ],
    "verbose": true,
    "moduleNameMapper": {
      "\\.(css|less)$": "less"
    }
  }
